.t10, .z11 {
  margin: 0;
  outline: 0;
  appearance: none
}

.x8, .z11 {
  transition: 375ms
}

.l14, .z11 {
  box-sizing: border-box
}

.cf, .e4, .t10, .z11 {
  font-size: 15px;
  line-height: 1.25rem
}

.f16, .z11 {
  cursor: pointer
}

.dc {
  display: grid;
  grid-template-areas:'before label after';
  grid-template-columns:1fr auto 1fr;
  align-items: center
}

.m13, .z11 {
  display: block
}

.dc:after, .dc:before {
  content: '';
  grid-area: line;
  display: block;
  border-top: 1px dashed var(--neutral-125);
  transform: translateY(50%)
}

.dc:before {
  grid-area: before
}

.dc:after {
  grid-area: after
}

.m13 {
  padding: 0 .5rem;
  grid-area: label;
  color: var(--neutral-400)
}

.g12, .l14 {
  grid-area: 1/1/2/2
}

.z11 {
  letter-spacing: -.015em;
  border: 0;
  font-weight: 600;
  border-radius: .5rem;
  padding: .875rem 1rem;
  position: relative;
  text-align: center;
  text-decoration: none
}

.k6, .r5, .x8 {
  display: grid
}

.s7 {
  width: 1.25rem;
  height: 1.25rem;
  position: absolute;
  right: 1rem;
  top: 50%;
  transform: translateY(-50%)
}

.wb {
  background: var(--primary-500);
  color: var(--contrast-primary-500)
}

.pe {
  background: var(--neutral-1000);
  color: var(--neutral-0)
}

.r5 {
  gap: .25rem
}

.e4 {
  letter-spacing: -.02em;
  font-weight: 700;
  color: var(--neutral-1000)
}

.t10 {
  letter-spacing: -.01em;
  border: 0;
  padding: .625rem 1rem;
  background: var(--neutral-50);
  border-radius: .5rem;
  color: var(--neutral-800)
}

.cf, .l14 {
  margin: auto
}

.k6 {
  gap: 1rem
}

.j3 {
  gap: .75rem
}

.x8 {
  position: fixed;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  backdrop-filter: blur(0.0625rem) saturate(0.75);
  font-family: -apple-system, BlinkMacSystemFont, "Segoe UI", Helvetica, Arial, sans-serif;
  -webkit-font-smoothing: antialiased
}

.g12 {
  content: '';
  background: var(--neutral-800);
  opacity: .8
}

.l14 {
  display: flex;
  flex-direction: column;
  position: relative;
  max-width: 48rem;
  width: 100%;
  background: #fff;
  border-radius: 1.5rem;
  max-height: calc(100vh - 6rem);
  transition: inherit
}

.s1 {
  max-width: 24rem
}

.k2 {
  display: grid;
  grid-template-areas:'. title close';
  grid-template-columns:1.5rem 1fr 1.5rem;
  padding: .75rem 1rem;
  border-bottom: 1px solid var(--neutral-100);
  color: var(--neutral-600)
}

.f16, .z0 {
  color: var(--neutral-800)
}

.z0 {
  padding: 1.5rem 2rem 2rem;
  overflow: auto
}

.id {
  border-top: 1px solid var(--neutral-100);
  display: none;
  padding: 1.5rem 2rem
}

.cf {
  grid-area: title
}

.f16 {
  grid-area: close;
  padding: 1rem;
  margin: -1rem;
  transition: 375ms;
  width: 1.5rem;
  height: 1.5rem
}

.t16, .ya {
  width: 100%
}

@media (hover: hover) {
  .wb:hover {
    background: var(--primary-700)
  }

  .pe:hover {
    background: var(--neutral-800)
  }

  .f16:hover {
    color: #000;
    transform: scale(1.125)
  }
}

.t16 {
  height: 100%;
  display: block
}

.x8:not(:target) {
  opacity: 0;
  pointer-events: none
}

.x8:not(:target) .l14 {
  transform: translateY(3rem)
}

@media (max-width: 32rem) {
  .l14 {
    max-height: calc(100vh - 3rem);
    margin-bottom: 0;
    border-bottom-left-radius: 0;
    border-bottom-right-radius: 0
  }

  .z0 {
    padding: 1.5rem 2rem
  }

  .id {
    display: block
  }
}

.k15 h1, .k15 h2, .k15 h3, .k15 h4, .k15 h5, .k15 h6 {
  margin-top: 2rem;
  margin-bottom: 1rem;
  color: var(--neutral-1000);
  font-weight: 600
}

.k15 h2 {
  font-size: 21px;
  line-height: 2rem
}

.k15 h3 {
  font-size: 19px;
  line-height: 1.75rem
}

.k15 h4 {
  font-size: 17px;
  line-height: 1.5rem
}

.k15 h5, .k15 ol, .k15 p, .k15 ul {
  font-size: 15px;
  line-height: 1.25rem
}

.k15 ol, .k15 p, .k15 ul {
  letter-spacing: -.01em;
  margin: 1rem 0
}

.k15 ol, .k15 ul {
  padding-left: 2rem
}

.k15 a {
  color: var(--primary-500)
}

.k15 .k6 {
  margin: 1.5rem 0
}

.k15 > :first-child {
  margin-top: 0
}

.k15 > :last-child {
  margin-bottom: 0
}
